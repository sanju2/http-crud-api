{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Sid": "MinimumGitLabPushMirroringPermissions",
          "Effect": "Allow",
          "Action": [
              "codecommit:GitPull",
              "codecommit:GitPush"
          ],
          "Resource": [
            "arn:aws:lambda:us-east-1:your-aws-account-number:function:your-lambda-function-name"
          ]
      }
  ]
}